<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mymap.map.com.webserviceretrofit">


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".firstkotlinapp.VolleySingleton"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".test2.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".firstkotlinapp.ViewArtistsActivity"/>



        <activity
            android:name=".test2.DetailActivity"
            android:label="details"
            android:parentActivityName=".test2.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".test2.MainActivity"/>
        </activity>
        <activity
            android:name=".test2.DetailActivityKotlin"
            android:label="details_kotlin"
            android:parentActivityName=".test2.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".test2.MainActivity"/>
        </activity>


    </application>

</manifest>